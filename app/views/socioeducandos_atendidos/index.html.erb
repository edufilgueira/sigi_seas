<%- model_class = SocioeducandoAtendido -%>
<% content_for :title do %>
  <%= I18n.t('activerecord.models.socioeducando_atendido', default: SocioeducandoAtendido.model_name.human.titleize) %>
<% end -%>
<div class="row">
  <div class="col-lg-12">
    <div class="ibox float-e-margins">
      <div class="ibox-title">
        <h5><%= I18n.t('activerecord.models.socioeducando_atendido', default: SocioeducandoAtendido.model_name.human.titleize) %></h5>
        <div class="ibox-tools">
          <a class="collapse-link">
            <i class="fa fa-chevron-up"></i>
          </a>
          <a class="close-link">
            <i class="fa fa-times"></i>
          </a>
        </div>
      </div>
      <div class="ibox-content">
        <div class="row">
          <div class="col-sm-12 b-r">
            <%= link_to_new new_socioeducando_atendido_path %>
            <table class="table table-hover table-sm">
              <thead>
                <tr>
                   <th><%= model_class.human_attribute_name(:ano) %></th>
                   <th><%= model_class.human_attribute_name(:mes) %></th>
                   <th><%= model_class.human_attribute_name(:unidade_socioeducativa_id) %></th>
                   <th><%= model_class.human_attribute_name(:quantidade) %></th>
                  <th width="18%"><%= I18n.t('helpers.actions') %></th>
                </tr>
              </thead>
              <tbody>
                <% @socioeducandos_atendidos.each do |socioeducando_atendido| %>
                  <tr>
                    <td><%= socioeducando_atendido.ano %></td>
                    <td><%= nome_mes(socioeducando_atendido.mes) %></td>
                    <td><%= socioeducando_atendido.unidade_socioeducativa&.sigla_nome %></td>
                    <td><%= socioeducando_atendido.quantidade %></td>
                    <td>
                      <%= link_to_show(socioeducando_atendido) %>
                      <%= link_to_edit(edit_socioeducando_atendido_path(socioeducando_atendido)) %>
                      <%= link_to_delete(socioeducando_atendido) %>
                    </td>
                  </tr>
                <% end %>
              </tbody>
            </table>

          </div>
        </div>
      </div>
    </div>
  </div>
</div>

