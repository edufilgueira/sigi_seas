                                    <td style="min-width: 90px; word-break: break-all;"><%= @socioeduk_medida_socioeducativa.inicio %></td>
                                    <td style="min-width: 90px; word-break: break-all;"><%= @socioeduk_medida_socioeducativa.fim_previsto %></td>
                                    <td style="min-width: 90px; word-break: break-all;"><%= @socioeduk_medida_socioeducativa.periodo_dias %></td>
                                  </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<%= link_to('Listar', socioeduk_medida_socioeducativas_path, class: 'btn btn-default') %>
<%= link_to 'Editar', edit_socioeduk_medida_socioeducativa_path(@socioeduk_medida_socioeducativa), class: 'btn btn-success' %>
