<%= form_for(@socioeduk_pedagogico_jovem_disciplina_escolaridade_medida, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_pedagogico_jovem_disciplina_escolaridade_medida %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_pedagogico_jovem_escolaridade_medida %>
      <%= f.collection_select :socioeduk_pedagogico_jovem_escolaridade_medida_id, Socioeduk::PedagogicoJovem.all, :id, :id, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_escolaridade %>
      <%= f.collection_select :socioeduk_escolaridade_id, Socioeduk::Escolaridade.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_situacao_disciplina %>
      <%= f.collection_select :socioeduk_situacao_disciplina_id, Socioeduk::SituacaoDisciplina.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :data_situacao %>
      <%= f.text_field :data_situacao, class: 'form-control datepicker' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :observacao %>
      <%= f.text_area :observacao, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_pedagogico_jovem_disciplina_escolaridade_medidas_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
