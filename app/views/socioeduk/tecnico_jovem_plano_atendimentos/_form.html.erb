<%= form_for(@socioeduk_tecnico_jovem_plano_atendimento, html: { class: 'form-horizontal', multipart: true }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_tecnico_jovem_plano_atendimento %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :tecnico_jovem_id %>
      <%= f.collection_select :tecnico_jovem_id, Socioeduk::TecnicoJovem.all, :id, :nome_tecnico, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :data_inclusao %>
      <%= f.text_field :data_inclusao, class: 'form-control datepicker' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :unidade_socioeducativa_inclusao_id %>
      <%= f.collection_select :unidade_socioeducativa_inclusao_id, UnidadeSocioeducativa.all, :id, :nome, {prompt: '---selecione---'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :anexo %>
      <%= f.file_field :anexo, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_tecnico_jovem_plano_atendimentos_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
