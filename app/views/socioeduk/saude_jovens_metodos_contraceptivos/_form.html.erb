<%= form_for(@socioeduk_saude_jovens_metodos_contraceptivo, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_saude_jovens_metodos_contraceptivo %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_saude_jovem %>
      <%= f.text_field :socioeduk_saude_jovem, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_metodo_contraceptivo %>
      <%= f.text_field :socioeduk_metodo_contraceptivo, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :outros_metodos %>
      <%= f.text_field :outros_metodos, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_saude_jovens_metodos_contraceptivos_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
