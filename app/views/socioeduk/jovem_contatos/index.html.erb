<%- model_class = Socioeduk::JovemContato -%>

<table class="table table-hover table-sm">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:socioeduk_tipo_contato) %></th>
      <th><%= model_class.human_attribute_name(:numero) %></th>
    </tr>
  </thead>
  <tbody>
    <% socioeduk_jovem_contatos.each do |socioeduk_jovem_contato| %>
    <tr>
      <td><%= socioeduk_jovem_contato.socioeduk_tipo_contato&.descricao %></td>
      <td><%= socioeduk_jovem_contato.numero %></td>
    </tr>
    <% end %>
  </tbody>
</table>