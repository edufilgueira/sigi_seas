<%= form_for(@socioeduk_tecnico_jovem_anexo, html: { class: 'form-horizontal', multipart: true }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_tecnico_jovem_anexo %>
  <%= render 'layouts/message_required' %>
 <div class="form-group">
    <div class="col-sm-4 col-offset-1">
      <%= f.label :data_inclusao %>
      <%= f.text_field :data_inclusao, class: 'form-control datepicker', required: "true" %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_tecnico_jovem %>
     <%= f.collection_select :socioeduk_tecnico_jovem_id, Socioeduk::TecnicoJovem.all, :id, :id, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :unidade_socioeducativa %>
      <%= f.collection_select :unidade_socioeducativa_id, UnidadeSocioeducativa.all, :id, :nome,  {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :anexo %>
      <%= f.file_field :anexo, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_tecnico_jovem_anexos_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
