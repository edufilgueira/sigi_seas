<%= form_for([@socioeduk_jovem, @socioeduk_contato], url: @url, remote:true, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_contato %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-3">
      <%= f.label :tipo_contato_id %>
      <%= f.collection_select :tipo_contato_id, Socioeduk::TipoContato.all, :id, :descricao, {}, class: 'form-control' %>
    </div>
    <div class="col-sm-2">
      <%= f.label :ddd %>
      <%= f.number_field :ddd, class: 'form-control ddd' %>
    </div>
    <div class="col-sm-3">
      <%= f.label :numero %>
      <%= f.text_field :numero, class: 'form-control telefone' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
    </div>
  </div>
<% end %>
