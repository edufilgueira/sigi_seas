<%= form_for(@socioeduk_saude_jovens_parentes_viciado, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_saude_jovens_parentes_viciado %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_saude_jovens %>
      <%= f.text_field :socioeduk_saude_jovens, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_tipo_parentesco %>
      <%= f.collection_select :socioeduk_tipo_parentesco_id, Socioeduk::TipoParentesco.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_entorpecente %>
      <%= f.collection_select :socioeduk_entorpecente_id, Socioeduk::Entorpecente.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_saude_jovens_parentes_viciados_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
