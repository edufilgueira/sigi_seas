<%= form_for(@socioeduk_saude_jovem_exame, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_saude_jovem_exame %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :saude_jovem %>
      <%= f.collection_select :saude_jovem, Socioeduk::SaudeJovem.all, :id, :descricao, {include_blank: '---selecione---'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_tipo_exame %>
      <%= f.collection_select :socioeduk_tipo_exame_id, Socioeduk::TipoExame.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :data_exame %>
      <%= f.text_field :data_exame, class: 'form-control datepicker' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_saude_jovem_exames_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
