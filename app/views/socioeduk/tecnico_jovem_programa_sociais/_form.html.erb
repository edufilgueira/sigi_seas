<%= form_for(@socioeduk_tecnico_jovem_programa_social, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_tecnico_jovem_programa_social %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_tecnico_jovem %>
      <%= f.collection_select :tecnico_jovem_id, Socioeduk::TecnicoJovem.all, :id, :id, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_programa_social %>
      <%= f.collection_select :socioeduk_programa_social_id, Socioeduk::ProgramaSocial.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_tecnico_jovem_programa_sociais_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
