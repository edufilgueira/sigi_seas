<%= form_for(@socioeduk_saude_jovem_vacina, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_saude_jovem_vacina %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :saude_jovem_id %>
      <%= f.text_field :saude_jovem_id, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_tipo_vacina %>
      <%= f.collection_select :socioeduk_tipo_vacina_id, Socioeduk::TipoVacina.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :data_vacinacao %>
      <%= f.text_field :data_vacinacao, class: 'form-control datepicker' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_saude_jovem_vacinas_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
