<%= form_for(@socioeduk_tecnico_jovem_visita_familiar, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_tecnico_jovem_visita_familiar %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_tecnico_jovem %>
      <%= f.collection_select :tecnico_jovem_id, Socioeduk::Jovem.all, :id, :id, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :data_visita %>
      <%= f.text_field :data_visita, class: 'form-control datepicker' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :pessoa_visitada %>
      <%= f.text_field :pessoa_visitada, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :finalidade_visita %>
      <%= f.text_area :finalidade_visita, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :responsavel_realizacao %>
      <%= f.text_field :responsavel_realizacao, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_tecnico_jovem_visita_familiares_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
