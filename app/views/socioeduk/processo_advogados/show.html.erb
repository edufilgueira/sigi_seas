<% content_for :title do %>
  <%= t('.title', default: @socioeduk_processo_advogado.class.model_name.human ) %>
<% end -%>
<div class="row">
  <div class="col-lg-12">
    <div class="ibox float-e-margins">
      <div class="ibox-title">
        <h5><%= t('.title', default: @socioeduk_processo_advogado.class.model_name.human ) %></h5>
        <div class="ibox-tools">
          <a class="collapse-link">
            <i class="fa fa-chevron-up"></i>
          </a>
          <a class="close-link">
            <i class="fa fa-times"></i>
          </a>
        </div>
      </div>
      <div class="ibox-content">
        <div class="row">
          <div class="col-sm-12 b-r">
            <table class="table table-condensed">
              <thead>
                <tr>
                                    
                  
                  <th><%= Socioeduk::ProcessoAdvogado.human_attribute_name :socioeduk_processo %></th>
                                    
                  
                  <th><%= Socioeduk::ProcessoAdvogado.human_attribute_name :nome %></th>
                                    
                  
                  <th><%= Socioeduk::ProcessoAdvogado.human_attribute_name :numero %></th>
                                    
                  
                  <th><%= Socioeduk::ProcessoAdvogado.human_attribute_name :uf_oab %></th>
                                    
                  
                  <th><%= Socioeduk::ProcessoAdvogado.human_attribute_name :defensor_publico %></th>
                                    
                  
                  <th><%= Socioeduk::ProcessoAdvogado.human_attribute_name :unidade_socioeducativa %></th>
                                  </tr>
              </thead>
              <tbody>
                <tr>
                                    <td style="min-width: 90px; word-break: break-all;"><%= @socioeduk_processo_advogado.socioeduk_processo %></td>
                                    <td style="min-width: 90px; word-break: break-all;"><%= @socioeduk_processo_advogado.nome %></td>
                                    <td style="min-width: 90px; word-break: break-all;"><%= @socioeduk_processo_advogado.numero %></td>
                                    <td style="min-width: 90px; word-break: break-all;"><%= @socioeduk_processo_advogado.uf_oab %></td>
                                    <td style="min-width: 90px; word-break: break-all;"><%= @socioeduk_processo_advogado.defensor_publico %></td>
                                    <td style="min-width: 90px; word-break: break-all;"><%= @socioeduk_processo_advogado.unidade_socioeducativa %></td>
                                  </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<%= link_to('Listar', socioeduk_processo_advogados_path, class: 'btn btn-default') %>
<%= link_to 'Editar', edit_socioeduk_processo_advogado_path(@socioeduk_processo_advogado), class: 'btn btn-success' %>
