<%- model_class = Socioeduk::ProcessoNatureza -%>
<% content_for :title do %>
<%=  I18n.t('activerecord.models.socioeduk_processo_natureza', default: Socioeduk::ProcessoNatureza.model_name.human.pluralize.titleize) %>
<% end -%>

<div class="row">
  <div class="col-lg-12">
    <div class="ibox float-e-margins">
      <div class="ibox-title">
        <h5><%=  I18n.t('activerecord.models.socioeduk_processo_natureza', default: Socioeduk::ProcessoNatureza.model_name.human.pluralize.titleize) %></h5>
        <div class="ibox-tools">
          <a class="collapse-link">
            <i class="fa fa-chevron-up"></i>
          </a>
          <a class="close-link">
            <i class="fa fa-times"></i>
          </a>
        </div>
      </div>
      <div class="ibox-content">
        <div class="row">
          <div class="col-sm-12 b-r">
            <%= link_to_new new_socioeduk_processo_natureza_path %>
            <table class="table table-hover table-sm">
              <thead>
                <tr>
                                    <th><%= model_class.human_attribute_name(:socioeduk_processo) %></th>
                                    <th><%= model_class.human_attribute_name(:tipo_natureza) %></th>
                                    <th><%= model_class.human_attribute_name(:artigo) %></th>
                                    <th width="18%"><%= I18n.t('helpers.actions') %></th>
                </tr>
              </thead>
              <tbody>
                <% @socioeduk_processo_naturezas.each do |socioeduk_processo_natureza| %>
                <tr>
                                    <td><%= socioeduk_processo_natureza.socioeduk_processo %></td>
                                    <td><%= socioeduk_processo_natureza.tipo_natureza %></td>
                                    <td><%= socioeduk_processo_natureza.artigo %></td>
                                    <td>
                    <%= link_to_show(socioeduk_processo_natureza) %>
                    <%= link_to_edit(edit_socioeduk_processo_natureza_path(socioeduk_processo_natureza)) %>
                    <%= link_to_delete(socioeduk_processo_natureza) %>
                  </td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
