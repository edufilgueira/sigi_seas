<%= form_for(@socioeduk_saude_jovem_atendimento, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_saude_jovem_atendimento %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-8 col-offset-1">
      <%= f.label :saude_jovem %>
      <%= f.collection_select :saude_jovem, Socioeduk::Jovem.all, :id, :nome, {include_blank: '--selecione--'}, class: 'form-control select2' %>
    </div>
    <div class="col-sm-2 col-offset-1">
      <%= f.label :data_atendimento %>
      <%= f.text_field :data_atendimento, class: 'form-control datepicker' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-5 col-offset-1">
      <%= f.label :socioeduk_tipo_atendimento %>
      <%= f.collection_select :socioeduk_tipo_atendimento_id, Socioeduk::TipoAtendimento.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
    <div class="col-sm-5 col-offset-1">
      <%= f.label :socioeduk_cid %>
      <%= f.collection_select :socioeduk_cid, Socioeduk::Cid.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
  </div>
  <div class="form-group">
    <div class="col-sm-5 col-offset-1">
      <%= f.label :socioeduk_local_atendimento %>
      <%= f.collection_select :local_atendimento, Socioeduk::LocalAtendimento.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
    <div class="col-sm-5 col-offset-1">
      <%= f.label :outro_local_atendimento %>
      <%= f.text_field :outro_local_atendimento, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-5 col-offset-1">
      <%= f.label :diagnostico %>
      <%= f.text_field :diagnostico, class: 'form-control' %>
    </div>
    <div class="col-sm-5 col-offset-1">
      <%= f.label :nome_profissional %>
      <%= f.text_field :nome_profissional, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :descricao_atendimento %>
      <%= f.text_field :descricao_atendimento, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_saude_jovem_atendimentos_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
