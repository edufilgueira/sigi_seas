<%= form_for(@socioeduk_pedagogico_jovem_disciplina_escolaridade, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_pedagogico_jovem_disciplina_escolaridade %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_pedagogico_jovem %>
      <%= f.collection_select :pedagogico_jovem_id, Socioeduk::PedagogicoJovem.all, :id, :id, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_disciplina_escolaridade %>
      <%= f.collection_select :socioeduk_disciplina_escolaridade_id, Socioeduk::DisciplinaEscolaridade.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_situacao_disciplina %>
      <%= f.collection_select :socioeduk_situacao_disciplina_id, Socioeduk::SituacaoDisciplina.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :data_conclusao %>
      <%= f.text_field :data_conclusao, class: 'form-control datepicker' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :observacao %>
      <%= f.text_area :observacao, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_pedagogico_jovem_disciplina_escolaridades_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
