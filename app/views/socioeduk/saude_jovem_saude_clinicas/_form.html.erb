<%= form_for(@socioeduk_saude_jovem_saude_clinica, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_saude_jovem_saude_clinica %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :saude_jovem_id %>
      <%= f.text_field :saude_jovem_id, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :odontologico_clinico %>
      <%= f.check_box :odontologico_clinico %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_cid %>
      <%= f.collection_select :socioeduk_cid_id, Socioeduk::Cid.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :diagnostico %>
      <%= f.text_field :diagnostico, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_local_tratamento %>
      <%= f.collection_select :socioeduk_local_tratamento_id, Socioeduk::LocalTratamento.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :outro_local_tratamento %>
      <%= f.text_field :outro_local_tratamento, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_saude_jovem_saude_clinicas_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
