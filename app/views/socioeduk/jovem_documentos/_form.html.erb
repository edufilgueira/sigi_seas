<%= form_for(@socioeduk_jovem_documento, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_jovem_documento %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_jovem %>
      <%= f.collection_select :socioeduk_jovem_id, Socioeduk::Jovem.all, :id, :nome_concat_mae, {include_blank: '--selecione--'}, class: 'form-control select2' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_tipo_documento %>
      <%= f.collection_select :socioeduk_tipo_documento_id, Socioeduk::TipoDocumento.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control select2' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :numero %>
      <%= f.text_field :numero, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-4 col-offset-1">
      <%= f.label :data_emissao %>
      <%= f.text_field :data_emissao,  class: 'form-control datepicker', 'data-autoclose': true, required: "true" %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :origem_documento %>
      <%= f.collection_select :unidade_socioeducativa, UnidadeSocioeducativa.all, :id , :nome, {prompt: "selecione"} ,{class: "form-control"} %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :destino_documento %>
      <%= f.collection_select :unidade_socioeducativa, UnidadeSocioeducativa.all, :id , :nome, {prompt: "selecione"} ,{class: "form-control"} %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_jovem_documentos_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
