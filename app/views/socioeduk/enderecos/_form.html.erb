<%= form_for([@jovem, @socioeduk_endereco], url: @url, remote: true, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_endereco %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-3">
      <%= f.label :tipo_contato_id %>
      <%= f.collection_select :tipo_contato_id, Socioeduk::TipoContato.all, :id, :descricao, {}, class: 'form-control' %>
    </div>
    <div class="col-sm-3">
      <%= f.label :cep %>
      <%= f.text_field :cep, class: 'form-control cep', data: {provider: :zipcode} %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.label :logradouro %>
      <%= f.text_field :logradouro, class: 'form-control', data: {provider: :street} %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-3">
      <%= f.label :numero %>
      <%= f.text_field :numero, class: 'form-control' %>
    </div>
    <div class="col-sm-7">
      <%= f.label :bairro %>
      <%= f.text_field :bairro, class: 'form-control', data: {provider: :neighborhood} %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-5">
      <%= f.label :complemento %>
      <%= f.text_field :complemento, class: 'form-control' %>
    </div>
    <div class="col-sm-5">
      <%= f.label :ponto_referencia %>
      <%= f.text_field :ponto_referencia, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-3">
      <%= f.label :cidade %>
      <%= f.text_field :cidade, class: 'form-control', data: {provider: :city} %>
    </div>
    <div class="col-sm-1">
      <%= f.label :estado %>
      <%= f.text_field :estado, class: 'form-control', data: {provider: :state} %>
    </div>
  </div>


  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
  </div>
<% end %>
