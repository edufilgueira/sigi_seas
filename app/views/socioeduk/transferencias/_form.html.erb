<%= form_for(@socioeduk_transferencia, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_transferencia %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :unidade_socioeducativa_origem_id %>
      <%= f.number_field :unidade_socioeducativa_origem_id, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :unidade_socioeducativa_destino_id %>
      <%= f.number_field :unidade_socioeducativa_destino_id, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :admissao %>
      <%= f.text_field :admissao, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_transferencias_path, class: 'btn' %>
    </div>
  </div>
<% end %>
