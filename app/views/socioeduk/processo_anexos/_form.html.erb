<%= form_for(@socioeduk_processo_anexo, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_processo_anexo %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_processo %>
      <%= f.collection_select :socioeduk_processo_id, Socioeduk::Processo.all, :id, :numero_processo,  {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :anexo %>
      <%= f.file_field :anexo, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_processo_anexos_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
