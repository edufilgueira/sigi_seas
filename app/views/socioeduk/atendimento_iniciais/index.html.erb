<%- model_class = Socioeduk::AtendimentoInicial -%>
<% content_for :title do %>
<%=  I18n.t('activerecord.models.socioeduk_atendimento_inicial', default: Socioeduk::AtendimentoInicial.model_name.human.pluralize.titleize) %>
<% end -%>

