                <th><%= model_class.human_attribute_name(:numero_oficio_bo) %></th>
                <th><%= model_class.human_attribute_name(:comarca_origem) %></th>
                <th><%= model_class.human_attribute_name(:procedencia) %></th>
                <th><%= model_class.human_attribute_name(:numero_processo) %></th>
