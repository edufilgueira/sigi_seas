                <th><%= model_class.human_attribute_name(:encaminhado_em) %></th>
                <th><%= model_class.human_attribute_name(:tipo_desligamento) %></th>
                <th><%= model_class.human_attribute_name(:desligado_em) %></th>
                <th><%= model_class.human_attribute_name(:deleted_at) %></th>
                <th width="18%"><%= I18n.t('helpers.actions') %></th>
