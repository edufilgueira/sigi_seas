<br>
<%= search_form_for @q do |f| %>
  <div class="form-group">
    <div class="row">
      <div class="col-sm-5">
        <%= f.search_field :socioeduk_jovem_nome_eq, placeholder: "Nome", class: "form-control" %>
      </div>
      <div class="col-sm-3">
        <%= f.collection_select :socioeduk_situacao_escolar_id_eq, Socioeduk::SituacaoEscolar.all, :id, :descricao, {include_blank: true, prompt: "Situação Escolar"}, class: "form-control" %>
      </div>

      <%= f.submit 'Buscar', class: "btn btn-primary" %>
      <%= link_to "Limpar",  socioeduk_pedagogico_jovens_path, class:"btn btn-warning" %>
    </div>
  </div>
<% end %>