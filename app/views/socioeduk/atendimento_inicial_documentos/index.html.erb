<%- model_class = Socioeduk::AtendimentoInicialDocumento -%>
<% content_for :title do %>
<%=  I18n.t('activerecord.models.socioeduk_atendimento_inicial_documento', default: Socioeduk::AtendimentoInicialDocumento.model_name.human.pluralize.titleize) %>
<% end -%>

<div class="row">
  <div class="col-lg-12">
    <div class="ibox float-e-margins">
      <div class="ibox-title">
        <h5><%=  I18n.t('activerecord.models.socioeduk_atendimento_inicial_documento', default: Socioeduk::AtendimentoInicialDocumento.model_name.human.pluralize.titleize) %></h5>
        <div class="ibox-tools">
          <a class="collapse-link">
            <i class="fa fa-chevron-up"></i>
          </a>
          <a class="close-link">
            <i class="fa fa-times"></i>
          </a>
        </div>
      </div>
      <div class="ibox-content">
        <div class="row">
          <div class="col-sm-12 b-r">
            <%= link_to_new new_socioeduk_atendimento_inicial_documento_path %>
            <table class="table table-hover table-sm">
              <thead>
                <tr>
                  <th><%= model_class.human_attribute_name(:descricao) %></th>
                  <th><%= model_class.human_attribute_name(:atendimento_inicial) %></th>
                  <th><%= model_class.human_attribute_name(:documento_forma_entrada) %></th>
                  <th><%= model_class.human_attribute_name(:documento) %></th>
                  <th width="18%"><%= I18n.t('helpers.actions') %></th>
                </tr>
              </thead>
              <tbody>
                <% @socioeduk_atendimento_inicial_documentos.each do |socioeduk_atendimento_inicial_documento| %>
                <tr>
                  <td><%= socioeduk_atendimento_inicial_documento.descricao %></td>
                  <td><%= socioeduk_atendimento_inicial_documento.atendimento_inicial %></td>
                  <td><%= socioeduk_atendimento_inicial_documento.documento_forma_entrada %></td>
                  <td><%= socioeduk_atendimento_inicial_documento.documento %></td>
                  <td>
                    <%= link_to_show(socioeduk_atendimento_inicial_documento) %>
                    <%= link_to_edit(edit_socioeduk_atendimento_inicial_documento_path(socioeduk_atendimento_inicial_documento)) %>
                    <%= link_to_delete(socioeduk_atendimento_inicial_documento) %>
                  </td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
