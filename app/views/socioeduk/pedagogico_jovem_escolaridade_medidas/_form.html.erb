<%= form_for(@socioeduk_pedagogico_jovem_escolaridade_medida, html: { class: 'form-horizontal' }) do |f| %>
  <%#= render 'layouts/error_messages', model_name: @socioeduk_pedagogico_jovem_escolaridade_medida %>
  <%= render 'layouts/message_required' %>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_pedagogico_jovem %>
      <%= f.collection_select :pedagogico_jovem_id, Socioeduk::PedagogicoJovem.all, :id, :id, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_escolaridade %>
      <%= f.collection_select :socioeduk_escolaridade_id, Socioeduk::Escolaridade.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :socioeduk_tipo_local_curso %>
      <%= f.collection_select :socioeduk_tipo_local_curso_id, Socioeduk::TipoLocalCurso.all, :id, :descricao, {include_blank: '--selecione--'}, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :escola %>
      <%= f.text_field :escola, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10 col-offset-1">
      <%= f.label :ano %>
      <%= f.number_field :ano, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <%= f.submit nil, class: 'btn btn-primary', data: { disable_with: disable_with } %>
      <%= link_to I18n.t('helpers.links.cancel'), socioeduk_pedagogico_jovem_escolaridade_medidas_path, class: 'btn btn-warning' %>
    </div>
  </div>
<% end %>
