<%- model_class = SocioedukAudiencia -%>
<% content_for :title do %>
<%=  I18n.t('activerecord.models.socioeduk_audiencia', default: SocioedukAudiencia.model_name.human.pluralize.titleize) %>
<% end -%>

<div class="row">
  <div class="col-lg-12">
    <div class="ibox float-e-margins">
      <div class="ibox-title">
        <h5><%=  I18n.t('activerecord.models.socioeduk_audiencia', default: SocioedukAudiencia.model_name.human.pluralize.titleize) %></h5>
        <div class="ibox-tools">
          <a class="collapse-link">
            <i class="fa fa-chevron-up"></i>
          </a>
          <a class="close-link">
            <i class="fa fa-times"></i>
          </a>
        </div>
      </div>
      <div class="ibox-content">
        <div class="row">
          <div class="col-sm-12 b-r">
            <%= link_to_new new_socioeduk_audiencia_path %>
            <table class="table table-hover table-sm">
              <thead>
                <tr>
                                    <th><%= model_class.human_attribute_name(:socioeduk_processo) %></th>
                                    <th><%= model_class.human_attribute_name(:socioeduk_comarca) %></th>
                                    <th><%= model_class.human_attribute_name(:numero_processo) %></th>
                                    <th><%= model_class.human_attribute_name(:datahora) %></th>
                                    <th><%= model_class.human_attribute_name(:observacao) %></th>
                                    <th width="18%"><%= I18n.t('helpers.actions') %></th>
                </tr>
              </thead>
              <tbody>
                <% @socioeduk_audiencias.each do |socioeduk_audiencia| %>
                <tr>
                                    <td><%= socioeduk_audiencia.socioeduk_processo %></td>
                                    <td><%= socioeduk_audiencia.socioeduk_comarca %></td>
                                    <td><%= socioeduk_audiencia.numero_processo %></td>
                                    <td><%= socioeduk_audiencia.datahora %></td>
                                    <td><%= socioeduk_audiencia.observacao %></td>
                                    <td>
                    <%= link_to_show(socioeduk_audiencia) %>
                    <%= link_to_edit(edit_socioeduk_audiencia_path(socioeduk_audiencia)) %>
                    <%= link_to_delete(socioeduk_audiencia) %>
                  </td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
