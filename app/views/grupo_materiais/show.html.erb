<div class="card">
  <div class="card-header">
    <%= t('.title', default: @grupo_material.class.model_name.human ) %>
  </div>
  <div class="card-body">
    <table class="table table-condensed">
      <thead>
      <tr>
                    <th><%= GrupoMaterial.human_attribute_name :nome %></th>
              </tr>
      </thead>
      <tbody>
      <tr>
                    <td style="min-width: 90px; word-break: break-all;"><%= @grupo_material.nome %></th>
              </tr>
      </tbody>
    </table>
  </div>
</div>

<%= link_to('Listar', grupo_materiais_path, class: 'btn btn-default') %>
<%= link_to 'Editar', edit_grupo_material_path(@grupo_material), class: 'btn btn-detran-primary' %>
